package com.csye6225.fall2018.coursepractise.datamodel;

public class Announcement {
	private String Id;
	private String announcementId;
	private String announcementText;
	private long boardId;
	
	public Announcement() {
		
	}
	
	public Announcement(String announcementId, String announcementText, long boardId) {
		this.setAnnouncementId(announcementId);
		this.setAnnouncementText(announcementText);
		this.setBoardId(boardId);
	}
	
	public String getId() {
		return Id;
	}
	public void setId(String id) {
		Id = id;
	}
	public String getAnnouncementId() {
		return announcementId;
	}
	public void setAnnouncementId(String announcementId) {
		this.announcementId = announcementId;
	}
	public String getAnnouncementText() {
		return announcementText;
	}
	public void setAnnouncementText(String announcementText) {
		this.announcementText = announcementText;
	}
	public long getBoardId() {
		return boardId;
	}
	public void setBoardId(long boardId) {
		this.boardId = boardId;
	}
}
